{
  "name": "recover",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "yarn run env -s && ng build --prod",
    "start": "yarn run env -s && ng serve --aot --proxy-config proxy.conf.json",
    "lint": "ng lint --type-check && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "ng test",
    "test:ci": "yarn run lint -s && ng test --single-run --code-coverage",
    "e2e": "yarn run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/flex-layout": "^5.0.0-beta.14",
    "@angular/forms": "^5.2.9",
    "@angular/http": "^5.2.9",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@angular/router": "^5.2.9",
    "@auth0/angular-jwt": "^1.1.0",
    "@ng-select/ng-select": "^0.35.1",
    "@ngrx/effects": "^5.2.0",
    "@ngrx/entity": "^5.2.0",
    "@ngrx/store": "^5.2.0",
    "@ngrx/store-devtools": "^5.2.0",
    "@ngx-translate/core": "^9.1.1",
    "@swimlane/ngx-datatable": "^11.2.0",
    "angular-in-memory-web-api": "^0.5.4",
    "chance": "^1.0.13",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "libphonenumber-js": "^1.1.7",
    "lodash": "^4.17.5",
    "material-design-icons": "^3.0.1",
    "ngrx-data": "^1.0.0-beta.3",
    "ngx-currency": "^1.1.1",
    "node-sass": "^4.8.3",
    "perfect-scrollbar": "^1.3.0",
    "rxjs": "^5.5.8",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/language-service": "^5.2.9",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@ngx-rocket/scripts": "ngx-rocket/scripts",
    "@types/jasmine": "^2.8.6",
    "@types/jasminewd2": "^2.0.3",
    "@types/lodash": "^4.14.106",
    "@types/node": "^9.6.2",
    "codelyzer": "^4.2.1",
    "hads": "^1.6.1",
    "htmlhint": "^0.9.13",
    "https-proxy-agent": "^2.2.1",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "^1.1.1",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "^5.3.1",
    "puppeteer": "^1.2.0",
    "stylelint": "~9.1.3",
    "stylelint-config-recommended-scss": "~3.1.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "~2.5.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "^2.8.1"
  }
}
